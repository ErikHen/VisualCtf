@page "/about"
@using VisualCtf.Shared.Services
@using VisualCtf.Shared.Models.CtfDelivery


@inject ICtfDeliveryService CtfDelivery

<MudPaper Elevation="0" Class="pa-5">
    <MudPaper Elevation="0" Class="mainbody" Style="max-width: 800px">
        @((MarkupString)_page.MainTextHtml) ERIK
    </MudPaper>
</MudPaper>

@code {
    private PageContent _page;

    protected override async Task OnInitializedAsync()
    {
        _page = await CtfDelivery.GetPage("/about");
    }
}